<%= simple_form_for @restaurant do |f| %>
  <%= f.input :name %>
  <%= f.input :address %>
   <%= f.input :phone_number %>
    <%= f.label :category %>
  <%= f.select :category, CATEGORY: ['chinese', 'italian', 'japanese', 'french', 'belgian'] %>
  <%= f.button :submit, class: "btn btn-dark" %>
<% end %>
